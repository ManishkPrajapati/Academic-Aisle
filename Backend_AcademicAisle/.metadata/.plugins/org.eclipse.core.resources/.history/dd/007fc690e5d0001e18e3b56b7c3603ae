package com.woodworks.project.models;


import org.springframework.beans.BeanUtils;

import com.woodworks.project.entities.Customer;
import com.woodworks.project.entities.OrderDetails;
import com.woodworks.project.entities.Product;

import lombok.AllArgsConstructor;
import lombok.Getter;
import lombok.NoArgsConstructor;
import lombok.Setter;


@Getter
@Setter
@AllArgsConstructor
@NoArgsConstructor
public class WishListDto {
	
	private int id;

	private Product product;
	
	private int qty;
	
	private Customer customer;
	
	public static OrderDetails toEntity(WishListDto dto) {
		OrderDetails order=new OrderDetails();
		// source to destination copied excluding pic parameter
		BeanUtils.copyProperties(dto, order);		
		return order	
	}
}
